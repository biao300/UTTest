{
    "compilerOptions": {
        "baseUrl": ".",
        "noImplicitAny": true,
        "noEmitOnError": true,
        "sourceMap": true,
        "module": "esnext",
        "moduleResolution": "node",
        "target": "es2019",
        "lib": ["es2015", "es2017", "es2019", "dom"],
        "jsx": "react",
        "strict": false,
        "downlevelIteration": true,
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
    },
    "exclude": [
        "webpack.config.js"
    ]
}